<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="2000020" id="com.jtronlabs.birdu" ios-CFBundleVersion="2.2.0" version="2.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Birdu</name>
    <description>
        It's a bird-eat-bird world out there, do what you must to survive.
    </description>
    <author email="jtronlabs@gmail.com" href="http://jtronlabs.github.io">
        James Lowrey (JTronLabs)
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="sms:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="16" />
        <preference name="android-manifest/application/activity/@android:theme" value="@android:style/Theme.Black.NoTitleBar" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="fullscreen" value="true" />
    <preference name="orientation" value="landscape" />
    <preference name="AndroidLaunchMode" value="singleInstance" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeepRunning" value="true" />
    <icon src="icon.png" />
    <engine name="android" spec="~5.2.2" />
    <engine name="ios" spec="~4.2.1" />
    <plugin name="cordova-plugin-console" spec="~1.0.3" />
    <preference name="xwalkVersion" value="20+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <plugin name="cordova-plugin-vibration" spec="~2.1.1" />
    <plugin name="cordova-plugin-dialogs" spec="~1.2.1" />
    <plugin name="cordova-plugin-x-socialsharing" spec="~5.1.3" />
    <plugin name="cc.fovea.cordova.purchase" spec="~6.0.0">
        <variable name="BILLING_KEY" value="" />
    </plugin>
    <plugin name="cordova-custom-config" spec="~2.0.3" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="~2.1.0">
        <variable name="XWALK_VERSION" value="21+" />
        <variable name="XWALK_LITEVERSION" value="xwalk_core_library_canary:17+" />
        <variable name="XWALK_COMMANDLINE" value="--disable-pull-to-refresh-effect" />
        <variable name="XWALK_MODE" value="embedded" />
        <variable name="XWALK_MULTIPLEAPK" value="true" />
    </plugin>
</widget>
